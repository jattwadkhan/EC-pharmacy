<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ecPharmacy">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.ecPharmacy.RatingActivity"></activity>
        <activity android:name="com.ecPharmacy.AddCardActivity" />
        <activity android:name="com.ecPharmacy.AddAddressActivity" />
        <activity android:name="com.ecPharmacy.CartActivity" />
        <activity android:name="com.ecPharmacy.HomeActivity" />
        <activity android:name="com.ecPharmacy.ForgotPasswordActivity" />
        <activity android:name="com.ecPharmacy.RegisterActivity" />
        <activity android:name="com.ecPharmacy.LoginActivity" />
        <activity android:name="com.ecPharmacy.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>